<img id = "<%= piece.id %>" class='piece-of-puzzle' src="/assets/<%= piece.title %>" height="<%= size %>" width="<%= size %>" style="transform:rotate(<%= piece.deviation %>deg);-webkit-transform:rotate(<%= piece.deviation %>deg);-moz-transform:rotate(<%= piece.deviation %>deg);position:absolute;left:<%=piece.x %>px;top:<%= piece.y %>px;">